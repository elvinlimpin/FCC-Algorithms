function sumAll(arr) {
  
  //initialize sum and get max and min numbers
  var result = 0;
  var max = Math.max(arr[0],arr[1]);
  var min = Math.min(arr[0],arr[1]);
  
  /* set for loop to add from the minimum
     to the maximum with one integer increments */
  for (var i = 0; i < (max - min + 1); i++)
    result += small + i;
  
  return result;
}

sumAll([1, 4]);
